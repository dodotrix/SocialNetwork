
@{
    ViewBag.Title = "DeleteAccountGet";
}

<h2>Are you sure you want to delete account ?</h2>

@using (Html.BeginForm("DeleteAccount", "Manage", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    @Html.AntiForgeryToken()

<div class="col-md-offset-2 col-md-10">
    <input type="submit" value="Yes" class="btn btn-default" id="changename" />
    @Html.ActionLink("No", "Index", "Home", new { @class = "btn btn-default" })
</div>
    
}

